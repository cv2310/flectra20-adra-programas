<?xml version="1.0" encoding="utf-8"?>
<flectra>
    <record id="view_users_form" model="ir.ui.view">
        <field name="name">res.users.form.inherit.analytic.account</field>
        <field name="model">res.users</field>
        <field name="inherit_id" ref="base.view_users_form"/>
        <field name="arch" type="xml">
            <notebook position="inside">
                <page name="analytic_accounts" string="Proyectos Asignados">
                    <field name="x_account_analytic_account_ids" widget="many2many" options="{'no_create': True}"/>
                </page>
            </notebook>
        </field>
    </record>
</flectra>
